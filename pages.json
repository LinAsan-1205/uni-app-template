{
	"easycom": {
		"^sakura-(.*)": "@/components/sakura-$1/sakura-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index"
	}, {
		"path": "pages/template/template",
		"style": {
			"navigationBarTitleText": "模版"
		}

	}, {
		"path": "pages/user/user",
		"style": {
			"navigationBarTitleText": "我的"
		}

	}, {
		"path": "pages/module/module",
		"style": {
			"navigationBarTitleText": "module"
		}

	}],
	"tabBar": {
		"list": [{
				"pagePath": "pages/index/index"
			}, {
				"pagePath": "pages/module/module"
			},
			{
				"pagePath": "pages/template/template"
			},
			{
				"pagePath": "pages/user/user"
			}
		]
	},
	"subPackages": [{
		"root": "pages/components",
		"pages": [{
				"path": "button/button",
				"style": {
					"navigationBarTitleText": "按钮",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "loading/loading",
				"style": {
					"navigationBarTitleText": "loading"
				}

			},
			{
				"path": "navbar/navbar",
				"style": {
					"navigationBarTitleText": "Navbar导航",
					"navigationStyle": "custom"
				}

			}, {
				"path": "search/search",
				"style": {
					"navigationBarTitleText": "Search搜索"
				}

			}, {
				"path": "icon/icon",
				"style": {
					"navigationBarTitleText": "icon 图标"
				}

			}, {
				"path": "avatar/avatar",
				"style": {
					"navigationBarTitleText": "Avatar 头像"
				}

			}, {
				"path": "link/link",
				"style": {
					"navigationBarTitleText": "Link 链接"
				}

			}, {
				"path": "text/text",
				"style": {
					"navigationBarTitleText": "Text 文本"
				}

			}, {
				"path": "cell/cell",
				"style": {
					"navigationBarTitleText": "Cell 单元格"
				}

			}, {
				"path": "badge/badge",
				"style": {
					"navigationBarTitleText": "Badge",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "tag/tag",
				"style": {
					"navigationBarTitleText": "Tag 标签"
				}

			}, {
				"path": "swiper/swiper",
				"style": {
					"navigationBarTitleText": "Swiper 轮播图"
				}

			}, {
				"path": "popup/popup",
				"style": {
					"navigationBarTitleText": "Popup 弹出层"
				}

			}, {
				"path": "picker/picker",
				"style": {
					"navigationBarTitleText": "Picker"
				}

			}, {
				"path": "dateTime/dateTime",
				"style": {
					"navigationBarTitleText": "DateTime"
				}

			}, {
				"path": "message/message",
				"style": {
					"navigationBarTitleText": "Message 消息"
				}

			}, {
				"path": "divider/divider",
				"style": {
					"navigationBarTitleText": "Divider 分割线"
				}

			}
		]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "sakura-ui",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom"
	},
	"uniIdRouter": {}
}
