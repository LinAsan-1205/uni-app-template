{
	"easycom": {
		"^sakura-(.*)": "@/components/sakura-$1/sakura-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "元素"
		}
	}, {
		"path": "pages/components/components",
		"style": {
			"navigationBarTitleText": "组件"
		}

	}, {
		"path": "pages/template/template",
		"style": {
			"navigationBarTitleText": "模版"
		}

	}, {
		"path": "pages/user/user",
		"style": {
			"navigationBarTitleText": "我的"
		}

	}],
	"tabBar": {
		"list": [{
				"pagePath": "pages/index/index"
			}, {
				"pagePath": "pages/components/components"
			},
			{
				"pagePath": "pages/template/template"
			},
			{
				"pagePath": "pages/user/user"
			}
		]
	},
	"subPackages": [{
		"root": "pages/components",
		"pages": [{
				"path": "/button/button",
				"style": {
					"navigationBarTitleText": "按钮",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "/loading/loading",
				"style": {
					"navigationBarTitleText": "loading"
				}

			},
			{
				"path": "/navbar/navbar",
				"style": {
					"navigationBarTitleText": "Navbar导航",
					"navigationStyle": "custom"
				}

			}, {
				"path": "/search/search",
				"style": {
					"navigationBarTitleText": "Search搜索"
				}

			}
		]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "sakura-ui",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
